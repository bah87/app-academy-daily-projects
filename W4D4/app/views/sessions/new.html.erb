<h2>User Login Page</h2>
<form action=" <%= session_url %> " method="post">
  <%= auth_token %>
  <label>Email
    <input type="text" name="user[email]">
  </label>
  <label>Password
    <input type="password" name="user[password]">
  </label>
  <input type="submit" name="name" value="Login">
</form>